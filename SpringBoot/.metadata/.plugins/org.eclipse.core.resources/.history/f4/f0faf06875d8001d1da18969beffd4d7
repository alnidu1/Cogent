package com.example.onetoonejpa2;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;

import com.example.onetoonejpa2.entity.Employee;
import com.example.onetoonejpa2.entity.Passport;
import com.example.onetoonejpa2.repository.EmployeeRepository;

@SpringBootApplication
public class OneToOneJpa2Application {

	@Autowired
	EmployeeRepository repo;
	public static void main(String[] args) {
		SpringApplication.run(OneToOneJpa2Application.class, args);
	}

	public void run(String...args) {
		Employee emp= new Employee(1, "name", "email", null);
		Passport p= new Passport(1, "addr", "exp", emp);
		repo.save(p);
	}
}
