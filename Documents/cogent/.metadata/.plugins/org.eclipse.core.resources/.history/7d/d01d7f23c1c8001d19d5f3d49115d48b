package poly;

class Car {
	int noOfGear;
	int drive(int speed) {
		
		return speed;
	}
	
	void display() {
		System.out.println("car");
	};
}

class SportCar extends Car{
	
	int AirBallonType=0;
	
	@Override
	void display() {
		System.out.println("Sportcar");
	}
}

public class car {
	public static void main(String args[]) {
		
		Car c= new Car();
		
		c.display();
		
		c=new SportCar();
		c.display();
		
		
	}
}
