package poly;

class Car {
	int noOfGear;
	int drive(int speed) {
		
		return speed;
	}
	
	void display() {
		System.out.println("car");
	};
}

class SportCar extends Car{
	
	int AirBallonType=0;
	
	@Override
	void display() {
		System.out.println("Sportcar");
	}
}

public class car {
	public static void main(String args[]) {
		
		Person p= new Person();
		p.behavior();
		
		p= new Parent();
		p.behavior();
		
		p= new Child();
		p.behavior();
		
		p= new Friend();
		p.behavior();
		
		p= new SO();
		p.behavior();
		
		
	}
}
